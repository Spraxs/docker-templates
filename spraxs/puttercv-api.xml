<?xml version="1.0" encoding="utf-8"?>
<Containers>
  <TemplateURL>https://raw.githubusercontent.com/Spraxs/docker-templates/master/spraxs/puttercv-api.xml</TemplateURL>
  <Beta>False</Beta>
  <Category>Web:API</Category>
  <Name>puttercv-api</Name>
  <Description>
	[b]Description[/b][br]
    REST API of PutterCV
  </Description>
  <Overview>
    REST API of PutterCV
  </Overview>
  <Registry>http://localhost:5000/</Registry>
  <Repository>localhost:5000/puttercv-api</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>8086</HostPort>
        <ContainerPort>80</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <WebUI>http://[IP]:[PORT:8086]/</WebUI>
  <Banner></Banner>
  <ExtraParams></ExtraParams>
</Containers>